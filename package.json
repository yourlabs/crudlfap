  "bugs": {
    "url": "https://github.com/yourlabs/crudlfap/issues"
  },
  "homepage": "https://github.com/yourlabs/crudlfap#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "browserslist": "^3.2.8",
    "css-loader": "^0.28.11",
    "debug": "^3.1.0",
    "enhanced-resolve": "^4.0.0",
    "eslint": "^4.18.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "form-serialize": "^0.7.2",
    "formdata-polyfill": "^3.0.10",
