    "stimulus": "^1.0.1",
    "turbolinks": "^5.1.1",
    "url-loader": "^1.0.1",
    "viewerjs": "^1.0.0",
    "webpack": "4.11.0",
    "webpack-cli": "^3.0.2",
    "whatwg-fetch": "^2.0.4",
    "yarn": "^1.7.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
